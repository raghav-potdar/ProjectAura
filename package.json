{
  "name": "aura",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && uvicorn app.main:app --reload --port 8000",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "dependencies": {
    "ical.js": "^2.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
